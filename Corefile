squic://.:10000 {
    tls ca/localhost/localhost-cert.pem ca/localhost/localhost-key.pem
    root ./zones
    rhine root.db . {
        scion on
    }
    log
    errors
}
squic://com.:10001 {
    tls ca/localhost/localhost-cert.pem ca/localhost/localhost-key.pem
    root ./zones
    rhine com.db com. {
        scion on
    }
    log
    errors
}

